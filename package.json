{
  "name": "baithani-volunteers-voting",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 5050",
    "build": "next build",
    "start": "next start",
    "check-type": "tsc --noEmit",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "prepare:husky": "husky",
    "db:generate:mysql": "drizzle-kit generate --config=./drizzle-mysql2.config.ts",
    "db:migrate:mysql": "drizzle-kit migrate --config=./drizzle-mysql2.config.ts",
    "db:seed:mysql": "node -r esbuild-register ./src/db/mysql2/seeder.ts",
    "db:prod:migrate:mysql": "NODE_ENV=production drizzle-kit migrate --config=./drizzle-mysql2.config.ts",
    "db:prod:seed:mysql": "NODE_ENV=production node -r esbuild-register ./src/db/mysql2/seeder.ts"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.7.2",
    "@emotion/cache": "^11.13.1",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@headlessui/react": "^2.2.0",
    "@mui/material": "^6.1.6",
    "@mui/material-nextjs": "^6.1.6",
    "@tanstack/react-query": "^5.59.16",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.35.3",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.3",
    "next": "15.0.1",
    "papaparse": "^5.4.1",
    "react": "19.0.0-rc-69d4b800-20241021",
    "react-dom": "19.0.0-rc-69d4b800-20241021",
    "react-hot-toast": "^2.4.1",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "drizzle-kit": "^0.26.2",
    "eslint": "^8",
    "eslint-config-next": "15.0.1",
    "husky": "^9.1.6",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
